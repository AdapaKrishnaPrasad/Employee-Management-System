<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" version="4.0">
  <display-name>employeemanagement</display-name>
  <servlet>
  	<servlet-name>login</servlet-name>
  	<servlet-class>employeemanagement.Login</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>login</servlet-name>
  	<url-pattern>/login</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>register</servlet-name>
  	<servlet-class>employeemanagement.Register</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>register</servlet-name>
  	<url-pattern>/register</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>registered</servlet-name>
  	<servlet-class>employeemanagement.CreateAccount</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>registered</servlet-name>
  	<url-pattern>/registersuccess</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>loggedin</servlet-name>
  	<servlet-class>employeemanagement.LoginSuccess</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>loggedin</servlet-name>
  	<url-pattern>/loginsuccess</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>home</servlet-name>
  	<servlet-class>employeemanagement.Home</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>home</servlet-name>
  <url-pattern>/Home</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>update</servlet-name>
  	<servlet-class>employeemanagement.Update</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>update</servlet-name>
  <url-pattern>/update</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>delete</servlet-name>
  	<servlet-class>employeemanagement.Delete</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>delete</servlet-name>
  <url-pattern>/delete</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>updatesuccessful</servlet-name>
  	<servlet-class>employeemanagement.UpdateSuccess</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>updatesuccessful</servlet-name>
  <url-pattern>/updatesuccess</url-pattern>
  </servlet-mapping>
</web-app>